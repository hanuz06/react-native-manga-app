{
  "expo": {
    "name": "My manga reader",
    "slug": "manga",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/manga-icon.png",
    "splash": {
      "image": "./assets/manga-spash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFB6C1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.hanuz.reactMangaReaderApp",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.hanuz.reactMangaReaderApp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/manga-spash.png",
        "backgroundColor": "#FFB6C1"
      }
    },
    "description": ""
  }
}
